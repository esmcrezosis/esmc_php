<form action="#" enctype="multipart/form-data" method="post" class="form-horizontal">
 <div class="control-group">
    <label class="control-label" for="code_group_ticket_support" style="font-weight:bold">
      Code Groupe *:
    </label>
    <div class="controls">
     <input type="text" name="code_group_ticket_support" class="input-block-level integrateur_demandeur_ticket_support" placeholder="Mettre ici le nom et prenoms du demandeur" required="required"/>
    </div>
 </div>
 <div class="control-group">
    <label class="control-label" for="integrateur_telephone_ticket_support" style="font-weight:bold">
       Libelle du Groupe(pour vous contacter):
    </label>
    <div class="controls">
    <input type="text" name="libelle_group_ticket_support" placeholder="Mettre ici le libelle du groupe" class="input-block-level integrateur_telephone_ticket_support"/>
    </div>
 </div>

       <div class="control-group" >
           <button type="submit" class="btn btn-large btn-primary">
           Valider le formulaire
           </button>
       </div>

</form>
